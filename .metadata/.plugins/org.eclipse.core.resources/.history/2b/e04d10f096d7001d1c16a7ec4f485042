package br.com.cursonjava.oo.classes;

public class Rectangle {
	int length, width;
	
	Rectangle(){ this(0, 0); }
	
	Rectangle(int length, int width){
		this.length = length;
		this.width = width;
	}

	private int getLength() { return length; }
	private int getWidth() { return width; }

	private void setLength(int length) { this.length = length; }
	private void setWidth(int width) { this.width = width; }
	
	public int getArea() { return getLength() * getWidth(); }
}