package br.com.cursonjava.oo.classes;

public class Shape {
	public String nome = "Forma";
	public double getArea() {
		return 0;
	}
	
	public String getNome() { return this.nome; }
	
	public void imprimeArea() {
		System.out.println(String.format("A área do %s é %f", this.getNome(), this.getArea()));
	}
}